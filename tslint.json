{
  "extends": ["tslint:latest", "tslint-react"],  
  "linterOptions": {
    "exclude": [
      "src/serviceWorker.ts"
    ]
  },
  "rules": {
    // override tslint-react rules here
    "quotemark": [true, "single", "jsx-single"],    
    "jsx-no-multiline-js": false,
    "no-empty": [true, "allow-empty-functions"],
    "jsx-alignment": false,
  }
}