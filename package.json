{
  "name": "ka-table",
  "version": "4.0.2",
  "license": "MIT",
  "repository": "github:komarovalexander/ka-table",
  "homepage": "https://komarovalexander.github.io/ka-table/#/overview",
  "dependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/react": "16.9.25",
    "@types/react-bootstrap": "^1.0.1",
    "@types/react-bootstrap-date-picker": "^4.0.6",
    "@types/react-redux": "^7.1.7",
    "bootstrap": "^4.5.0",
    "react": "^16.13.1",
    "react-bootstrap": "^1.2.2",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:all": "react-scripts test --watchAll",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "tslint --fix -c tslint.json 'src/**/*.{ts,tsx}'",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "dist": "tsc --declaration --jsx react --esModuleInterop true --outDir dist src/lib/index.ts && gulp build",
    "pack": "gulp build && cd dist && npm pack && cd .."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "25.1.4",
    "@types/node": "13.9.2",
    "@types/react-dom": "16.9.5",
    "@types/react-highlight": "^0.12.2",
    "@types/react-router-dom": "^5.1.3",
    "@types/simulant": "^0.2.0",
    "coveralls": "^3.0.11",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "gulp": "^4.0.2",
    "gulp-file": "^0.4.0",
    "gulp-gh-pages": "^0.5.4",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-typescript": "^5.0.1",
    "jsonfile": "^6.0.1",
    "node-sass": "^4.13.1",
    "react-filter-control": "^2.1.0",
    "react-ga": "^2.7.0",
    "react-highlight": "^0.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.1",
    "simulant": "^0.2.2",
    "tslint": "^6.1.0",
    "tslint-react": "^4.2.0",
    "typescript": "3.8.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/lib/Components/**/*.{ts,tsx}",
      "src/lib/Utils/*.ts"
    ]
  },
  "keywords": [
    "datagrid",
    "react",
    "table",
    "component",
    "widget"
  ]
}
